<table>
  <thead>
    <th>id</th>
    <th>status</th>
    <th>title</th>
    <th>description</th>
    <th>code</th>
    <th>price</th>
    <th>stock</th>
    <th>category</th>
    <th>thumbnails</th>
  </thead>
  <tbody>
    {{#each products}}
        <tr>
            <td> {{id}}</td>
            <td> {{status}}</td>
            <td> {{title}}</td>
            <td> {{description}}</td>
            <td> {{code}}</td>
            <td> {{price}}</td>
            <td> {{stock}}</td>
            <td> {{category}}</td>
            <td> {{thumbnails}}</td>
        </tr>
    {{/each}}

  </tbody>
</table>